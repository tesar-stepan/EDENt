<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class dynamic-insert="false" dynamic-update="false" mutable="true" name="edent.model.ToothState" optimistic-lock="version" polymorphism="implicit" select-before-update="false">
        <id name="id" type="java.lang.Long">
            <generator class="native"/>
        </id>
        <property name="name" type="string">
            <column name="name" not-null="true"/>
        </property>
        <property name="mark" type="string">
            <column name="mark" not-null="false"/>
        </property>
        <property name="icon" type="string">
            <column name="icon" not-null="false"/>
        </property>
        <property name="over" type="string">
            <column name="overX" not-null="false"/>
        </property>
    </class>
</hibernate-mapping>
